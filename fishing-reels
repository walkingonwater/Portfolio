		<script type="text/javascript">
			dojo.addOnLoad(function() { 

			var broswerWidth = window.innerWidth;
			var gridLength =  dojo.query(".product_listing_container > .grid_mode").children().length;
		
			if (broswerWidth >= 601 && gridLength != 0){
				console.log("PLP grid and width is greater than mobile");
				var productArray = dojo.query(".product_listing_container > .grid_mode").children();
				var columns=4;
				if (broswerWidth >= 601 && broswerWidth <= 725){
					columns=2;
				} else if (broswerWidth >= 726 && broswerWidth <= 1400){
					columns=3;
				}
				for (var i=0;i<productArray.length;i=i+columns){
					var maxHeight=productArray[i].clientHeight;
					var iplus1=i+1;
					for (var j=iplus1;j<i+columns && j<productArray.length;j++){
						maxHeight=Math.max(maxHeight,productArray[j].clientHeight);
						
					}
					console.log("row "+(iplus1)+" is "+maxHeight);
					//now i have the max height assign it
					for (var j=i;j<i+columns && j<productArray.length;j++){
						productArray[j].style.height = maxHeight+"px";
					}
				}
			}

			});
			
		</script>
		

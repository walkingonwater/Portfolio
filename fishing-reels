							<script>
								function getSelectedFacets() {
									var facetForm = document.forms['productsFacets'] != null ? document.forms['productsFacets'] : document.forms['productsFacetsHorizontal'];
									var facetOptions = facetForm.elements;
									var selectedFacets = [];
									
									for(var i = 0; i < facetOptions.length; i++){
										if(facetOptions[i].type != null && facetOptions[i].type.toLowerCase() == "checkbox" && facetOptions[i].checked) {
											selectedFacets.push(facetOptions[i].value);
										}
									}
									
									return selectedFacets;
								}							
								dojo.addOnLoad(function(){
									dojo.topic.subscribe("Facet_Add", function(){
										if (arguments.length > 0) {
											SearchBasedNavigationDisplayJS.appendFilterFacet(arguments[0]);
										} else {
											SearchBasedNavigationDisplayJS.appendFilterPriceRange('$');
										}
										SearchBasedNavigationDisplayJS.doSearchFilter();
										dojo.publish("CIRefreshProductOverlay");										
										analyticsDataLayer["refinements"] = getSelectedFacets();
									});
									dojo.topic.subscribe("Facet_Remove", function(){
									if (arguments.length > 0) {
											SearchBasedNavigationDisplayJS.removeFilterFacet(arguments[0]);
											SearchBasedNavigationDisplayJS.doSearchFilter();											
										} else {
											SearchBasedNavigationDisplayJS.removeFilterPriceRange();
										}
										dojo.publish("CIRefreshProductOverlay");										
										analyticsDataLayer["refinements"] = getSelectedFacets();
									});
									dojo.topic.subscribe("Facet_RemoveAll", function(){
										SearchBasedNavigationDisplayJS.clearAllFacets(true);
										dojo.publish("CIRefreshProductOverlay");										
										analyticsDataLayer["refinements"] = getSelectedFacets();
									});
								});
								analyticsDataLayer["resultCount"] = "293";
								analyticsDataLayer["pageNumber"] = "1";
								analyticsDataLayer["refinements"] = [];
								
								dojo.addOnLoad(function(data){
									dojo.subscribe("showResultsForPageNumber", function(data){
										if(data && data.pageNumber){
											analyticsDataLayer["pageNumber"] = data.pageNumber;
										}
									});
								})
							</script>
							

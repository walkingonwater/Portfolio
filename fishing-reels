	var cartId = "";
	var cartIdCookie = document.cookie.match(/WC_CartOrderId_715838534=[0-9]*/);
	
	if(cartIdCookie != null && cartIdCookie.length > 0 && cartIdCookie[0] != "" && cartIdCookie[0].indexOf("=") > -1){		
